{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-east5",
      "maxInstances": 10
    },
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "branding-immersion",
          "region": "us-east5"
        }
      }
    ]
  },
  "apphosting": {
    "backendId": "branding-immersion",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  }
}
